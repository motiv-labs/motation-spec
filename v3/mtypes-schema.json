{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "https://json-schema.org/draft/2019-09/output/schema",
  "description": "A schema that validates the structure of a mtypes",

  "type": "object",
  "required": [ "contentType" ],

  "properties": {
    "contentType": {
      "type": "array",
      "items": {
        "$ref": "mtype-schema.json"
      },
      "minItems": 1,
      "uniqueItems": true
    }
  }

}